From dc9582b150eb94385b260bb1ae527f69ef62014d Mon Sep 17 00:00:00 2001
From: bkardosa <bkardos70@gmail.com>
Date: Fri, 25 Mar 2022 13:50:27 +0100
Subject: [PATCH] add dts support for FRU EEPROM + GPIO expander on CPU board

---
 arch/arm/boot/dts/aspeed-ast2500-pcb486.dts | 20 ++++++++++++++------
 1 file changed, 14 insertions(+), 6 deletions(-)

diff --git a/arch/arm/boot/dts/aspeed-ast2500-pcb486.dts b/arch/arm/boot/dts/aspeed-ast2500-pcb486.dts
index 57330ab74195..3f68168e262b 100644
--- a/arch/arm/boot/dts/aspeed-ast2500-pcb486.dts
+++ b/arch/arm/boot/dts/aspeed-ast2500-pcb486.dts
@@ -110,22 +110,30 @@ &mac1 {
 	pinctrl-0 = <&pinctrl_rgmii2_default &pinctrl_mdio2_default>;
 };
 
-&i2c3 {
+&i2c6 {
 	status = "okay";
 
 	eeprom@50 {
-		compatible = "atmel,24c08";
+		compatible = "atmel,24c64";
 		reg = <0x50>;
-		pagesize = <16>;
+		pagesize = <32>;
 	};
 };
 
 &i2c7 {
 	status = "okay";
 
-	lm75@4d {
-		compatible = "national,lm75";
-		reg = <0x4d>;
+	eeprom@50 {
+		compatible = "atmel,24c64";
+		reg = <0x50>;
+		pagesize = <32>;
+	};
+
+	gpio@70 {
+		compatible = "nxp,pca9538";
+		gpio-controller;
+		#gpio-cells = <2>;
+		reg = <0x70>;
 	};
 };
 
